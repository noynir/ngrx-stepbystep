{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "base": {
      "root": "src",
      "sourceRoot": "src/base",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "src/base/index.html",
            "main": "src/base/main.ts",
            "tsConfig": "src/base/tsconfig.app.json",
            "polyfills": "src/base/polyfills.ts",
            "assets": [
              "src/base/assets",
              "src/base/favicon.ico"
            ],
            "styles": [
              "src/base/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/base/environments/environment.ts",
                  "with": "src/base/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "base:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "base:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "base:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/base/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/base/polyfills.ts",
            "tsConfig": "src/base/tsconfig.spec.json",
            "scripts": [],
            "styles": [
              "src/base/styles.scss"
            ],
            "assets": [
              "src/base/assets",
              "src/base/favicon.ico"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "base-e2e": {
      "root": "src",
      "sourceRoot": "src/e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "base:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "e2e/tsconfig.e2e.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "ngrx": {
      "root": "src",
      "sourceRoot": "src/ngrx",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "src/ngrx/index.html",
            "main": "src/ngrx/main.ts",
            "tsConfig": "src/ngrx/tsconfig.app.json",
            "polyfills": "src/ngrx/polyfills.ts",
            "assets": [
              "src/ngrx/assets",
              "src/ngrx/favicon.ico"
            ],
            "styles": [
              "src/ngrx/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/ngrx/environments/environment.ts",
                  "with": "src/ngrx/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ngrx:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ngrx:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ngrx:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/ngrx/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/ngrx/polyfills.ts",
            "tsConfig": "src/ngrx/tsconfig.spec.json",
            "scripts": [],
            "styles": [
              "src/ngrx/styles.scss"
            ],
            "assets": [
              "src/ngrx/assets",
              "src/ngrx/favicon.ico"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "ngrx-e2e": {
      "root": "src",
      "sourceRoot": "src/e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "ngrx:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "e2e/tsconfig.e2e.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "base",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "app",
      "styleext": "scss"
    },
    "@schematics/angular:directive": {
      "prefix": "app"
    }
  }
}